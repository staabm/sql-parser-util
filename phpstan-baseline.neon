			message: "#^Method staabm\\\\SqlParserUtils\\\\SQLParserUtils\\:\\:getNamedPlaceholderPositions\\(\\) should return array\\<int, string\\> but returns array\\.$#"
			count: 1
			path: src/SQLParserUtils.php


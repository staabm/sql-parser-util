{
  "name": "staabm/sql-parser-util",
  "license": "MIT",
  "require": {
    "php": "^7.2 || ^8.0"
  },
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "^1.3",
    "friendsofphp/php-cs-fixer": "3.4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "staabm\\SqlParserUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "staabm\\SqlParserUtils\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "csfix": [
      "php-cs-fixer fix"
    ],
    "test": [
      "phpunit"
    ]
  },
  "config": {
    "sort-packages": true
  }
}